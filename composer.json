{
  "name": "dotkernel/dot-paginator",
  "type": "library",
  "description": "Paginator component for dotkernel based on zend-paginator",
  "license": "MIT",
  "authors": [
    {
      "name": "n3vrax",
      "email": "tibi@apidemia.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "container-interop/container-interop": "^1.1",
    "zendframework/zend-servicemanager": "^3.0",
    "zendframework/zend-paginator": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3",
    "zendframework/zend-db": "^2.8",

    "dotkernel/dot-ems": "dev-reworked as 0.6.x-dev",
    "dotkernel/dot-hydrator": "0.6.x-dev",
    "dotkernel/dot-event": "dev-develop as 0.6.x-dev",
    "dotkernel/dot-helpers": "dev-develop as 0.6.x-dev"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-ems"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-hydrator"
    }
  ],
  "autoload": {
    "psr-4": {
      "Dot\\Paginator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\Paginator\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.6-dev"
    }
  }
}
